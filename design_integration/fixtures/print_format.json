  "html": "<div style=\"text-align:left; font-size:16px; font-weight:bold;margin:0; padding:0;\">Design Request Item</div><br>\n<div style=\"text-align:left; font-size:12px;color:#52595D;margin:0; padding:0;\">{{doc.name}}</div>\n<hr style=\"border:0; border-top:1px solid #000; margin:6px 0;\">\n<table style=\"width:100%; border-collapse: collapse;border:none; font-size: 12px;\">\n      <tbody>\n        <tr>\n            <td style=\"width:25%;\" ><b>Item Code:</b></td>\n            <td style=\"width:25%;\" >{{ doc.item_code }}</td>\n            <td style=\"width:25%;\" >{{ doc.item_name }}</td>\n            <td style=\"width:25%;\" ></td>\n        </tr>\n        <tr>\n            <td style=\"width:25%;\" ><b>Item Name:</b></td>\n            <td style=\"width:25%;\" >{{ doc.item_name }}</td>\n            <td style=\"width:25%;\" ><b>Company:</b></td>\n            <td style=\"width:25%;\" >{{ doc.company }}</td>\n        </tr>\n        <tr>\n            <td colspan=\"2\" style=\"width:20%;\" ><b>Quantity:</b></td>\n            <td colspan=\"2\" style=\"width:80%;\">{{ doc.qty }}</td>\n        </tr>\n        <tr><td colspan=\"2\" style=\"width:20%;\" ><b>UOM:</b></td>\n            <td colspan=\"2\" style=\"width:80%;\" >{{doc.uom}}</td>\n        </tr>\n        <tr>\n            <td style=\"width:25%;\" ><b>Design Status:</b></td>\n            <td style=\"width:25%;\" >{{ doc.design_status }}</td>\n            <td style=\"width:25%;\" ><b>Current Stage:</b></td>\n            <td style=\"width:25%;\" >{{ doc.current_stage }}</td>\n        </tr>\n        <tr>\n            <td style=\"width:25%;\" ><b>Approval Status:</b></td>\n            <td style=\"width:25%;\" >{{doc.approval_status}}</td>\n            <td style=\"width:25%;\" ><b>Approval Date:</b></td>\n            <td style=\"width:25%;\" >{{doc.approval_date}}</td>\n        </tr>\n        <tr>\n            <td colspan=\"2\" style=\"width:20%;\" ><b>Assigned To:</b></td>\n            <td colspan=\"2\" style=\"width:80%;\" >\n                {% set todos = frappe.get_all(\"ToDo\",\n    filters={\n        \"reference_type\": doc.doctype,\n        \"reference_name\": doc.name,\n        \"status\": [\"!=\", \"Cancelled\"]\n    },\n    fields=[\"allocated_to\"]\n) %}\n\n{% if todos %}\n    {% for t in todos %}\n        {{ t.allocated_to }}{% if not loop.last %}, {% endif %}\n    {% endfor %}\n{% else %}\n    {{\"\"}}\n{% endif %}\n</td>\n        </tr>\n        <tr><td colspan=\"2\" style=\"width:20%;\" ><b>Start Date:</b></td>\n            <td colspan=\"2\" >{{doc.start_date}}</td>\n        </tr>\n        <tr>\n            <td></td><td></td><td><b>Revision Count:</b></td>\n            <td >{{doc.revision_count}}</td>\n            </tr>\n    </tbody>\n</table>\n\n<table class=\"table table-bordered\" style=\"width:100%; border-collapse: collapse; font-size: 12px;\">\n    <thead>\n        <tr>\n            <th style=\"width:10%;\">Sr</th>\n            <th style=\"width:10%;\" >Stage</th>\n            <th style=\"width:10%;\" >From Status</th>\n            <th style=\"width:10%;\" >To Status</th>\n            <th style=\"width:20%;\" >Transition Date</th>\n            <th style=\"width:20%;\" >Transitioned By</th>\n            <th style=\"width:20%;\" >Remarks</th>\n        </tr>\n    </thead>\n    <tbody>\n        {% for row in doc.stage_transition_log %}\n        <tr>\n            <td>{{ loop.index }}</td>     \n            <td>{{ row.stage }}</td>     \n            <td>{{ row.from_status }}</td>\n            <td>{{ row.to_status }}</td>\n            <td>{{ row.transition_date }}</td>\n            <td>{{ row.transitioned_by }}</td>\n            <td>{{ row.remarks }}</td>\n        </tr>\n        {% else %}\n        <tr>\n            <td colspan=\"7\" style=\"text-align:center;\">\n                {{\" \"}}\n            </td>\n        </tr>\n        {% endfor %}\n    </tbody>\n</table>\n<table><tr style=\"width:100%; border-collapse: collapse;border:none; font-size: 12px;\">\n    <td style=\"width:50%;\"><b>Design Request:</b></td>\n    <td style=\"width:50%;\">{{doc.design_request}}</td>\n</tr></table> \n\n\n\n\n\n\n\n\n",
